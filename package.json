{
  "name": "citest",
  "version": "1.0.0",
  "description": "MicroService for CI TEST",
  "author": "Srikanth Indrakanti",
  "main": "src/index.js",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "dev:worker": "nodemon ./src/worker.js",
    "dev:stream": "nodemon ./src/stream.js",
    "lint": "eslint '**/*.js' --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "postpublish": "git push --tags",
    "docs": "apidoc -i src -o docs",
    "postdocs": "open-cli docs/index.html",
    "deploy:dev": "shipit dev deploy",
    "deploy:qa": "shipit qa deploy",
    "deploy:uat": "shipit uat deploy",
    "deploy:prod": "shipit production deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "eslint",
    "REST",
    "API",
    "citest"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ectosense/CITest.git"
  },
  "bugs": {
    "url": "https://github.com/ectosense/CITest/issues"
  },
  "homepage": "https://github.com/ectosense/CITest#readme",
  "dependencies": {
    "@hapi/joi-date": "^2.0.1",
    "@sentry/node": "^5.12.3",
    "agenda": "^2.3.0",
    "aws-sdk": "^2.741.0",
    "base64-arraybuffer": "^0.2.0",
    "celebrate": "^11.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv-safe": "^6.1.0",
    "elastic-apm-node": "^3.3.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.2",
    "helmet": "^3.21.2",
    "http-status": "^1.4.2",
    "jmespath": "^0.15.0",
    "knox": "^0.9.2",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.8.11",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sequelize": "^5.21.3",
    "uuidv4": "^6.2.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "sequelize-cli": "^5.5.1"
  }
}
